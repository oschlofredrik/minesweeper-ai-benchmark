{
  "version": 2,
  "builds": [
    {
      "src": "vercel/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/join",
      "dest": "/api/join.py"
    },
    {
      "src": "/host",
      "dest": "/api/host.py"
    },
    {
      "src": "/compete",
      "dest": "/api/compete.py"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel/api/index.py"
    }
  ]
}