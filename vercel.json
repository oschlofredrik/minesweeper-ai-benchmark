{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/join.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/compete.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/host.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/sessions.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/play.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/evaluations.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/admin.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/prompts.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/admin-page.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/events.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/run_game.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/benchmark.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/sessions_v2.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/realtime.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/leaderboard.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/join",
      "dest": "/api/join.py"
    },
    {
      "src": "/host",
      "dest": "/api/host.py"
    },
    {
      "src": "/compete",
      "dest": "/api/compete.py"
    },
    {
      "src": "/admin",
      "dest": "/api/admin-page.py"
    },
    {
      "src": "/api/sessions(.*)",
      "dest": "/api/sessions.py"
    },
    {
      "src": "/api/play(.*)",
      "dest": "/api/play.py"
    },
    {
      "src": "/api/evaluations(.*)",
      "dest": "/api/evaluations.py"
    },
    {
      "src": "/api/admin(.*)",
      "dest": "/api/admin.py"
    },
    {
      "src": "/api/prompts(.*)",
      "dest": "/api/prompts.py"
    },
    {
      "src": "/api/events",
      "dest": "/api/events.py"
    },
    {
      "src": "/api/run_game",
      "dest": "/api/run_game.py"
    },
    {
      "src": "/benchmark",
      "dest": "/api/benchmark.py"
    },
    {
      "src": "/api/sessions/(.*)/stream",
      "dest": "/api/realtime.py"
    },
    {
      "src": "/leaderboard",
      "dest": "/api/leaderboard.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}