{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/sessions.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/play.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/evaluations.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/admin.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/prompts.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/events.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/tasks.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/auth.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/sessions(.*)",
      "dest": "/api/sessions.py"
    },
    {
      "src": "/api/play(.*)",
      "dest": "/api/play.py"
    },
    {
      "src": "/api/evaluations(.*)",
      "dest": "/api/evaluations.py"
    },
    {
      "src": "/api/admin(.*)",
      "dest": "/api/admin.py"
    },
    {
      "src": "/api/prompts(.*)",
      "dest": "/api/prompts.py"
    },
    {
      "src": "/api/events",
      "dest": "/api/events.py"
    },
    {
      "src": "/api/tasks(.*)",
      "dest": "/api/tasks.py"
    },
    {
      "src": "/api/auth(.*)",
      "dest": "/api/auth.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}