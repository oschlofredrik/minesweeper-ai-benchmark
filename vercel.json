{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/sessions(.*)",
      "dest": "/api/sessions.py"
    },
    {
      "src": "/api/play(.*)",
      "dest": "/api/play.py"
    },
    {
      "src": "/api/benchmark(.*)",
      "dest": "/api/play.py"
    },
    {
      "src": "/api/evaluate-sdk(.*)",
      "dest": "/api/evaluate-sdk.py"
    },
    {
      "src": "/api/evaluation/(.*)",
      "dest": "/api/evaluation/[id]/route.ts"
    },
    {
      "src": "/api/auth(.*)",
      "dest": "/api/auth.py"
    },
    {
      "src": "/api/leaderboard(.*)",
      "dest": "/api/leaderboard.py"
    },
    {
      "src": "/api/run_game(.*)",
      "dest": "/api/run_game.py"
    },
    {
      "src": "/api/games(.*)",
      "dest": "/api/games_config.py"
    },
    {
      "src": "/api/game_runner(.*)",
      "dest": "/api/game_runner.py"
    },
    {
      "src": "/api/models(.*)",
      "dest": "/api/models_config.py"
    },
    {
      "src": "/api/stats",
      "dest": "/api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/api/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "functions": {
    "api/evaluation/[id]/route.ts": {
      "maxDuration": 300
    }
  }
}