{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/sessions_simple.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/play_simple.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/auth.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/simple.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/leaderboard_simple.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/run_game_simple.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/games_config.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/game_runner.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/models_config.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/ai_models.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/test_openai.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/test_deps.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/sessions(.*)",
      "dest": "/api/sessions_simple.py"
    },
    {
      "src": "/api/play(.*)",
      "dest": "/api/play_simple.py"
    },
    {
      "src": "/api/auth(.*)",
      "dest": "/api/auth.py"
    },
    {
      "src": "/api/simple",
      "dest": "/api/simple.py"
    },
    {
      "src": "/api/leaderboard",
      "dest": "/api/leaderboard_simple.py"
    },
    {
      "src": "/api/run_game",
      "dest": "/api/run_game_simple.py"
    },
    {
      "src": "/api/games(.*)",
      "dest": "/api/games_config.py"
    },
    {
      "src": "/api/game_runner(.*)",
      "dest": "/api/game_runner.py"
    },
    {
      "src": "/api/models(.*)",
      "dest": "/api/models_config.py"
    },
    {
      "src": "/api/benchmark(.*)",
      "dest": "/api/play_simple.py"
    },
    {
      "src": "/api/test_deps",
      "dest": "/api/test_deps.py"
    },
    {
      "src": "/api/test_openai",
      "dest": "/api/test_openai.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}