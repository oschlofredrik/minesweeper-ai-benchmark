{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/sessions_simple.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/play_simple.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/auth.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/simple.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/leaderboard_simple.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/sessions(.*)",
      "dest": "/api/sessions_simple.py"
    },
    {
      "src": "/api/play(.*)",
      "dest": "/api/play_simple.py"
    },
    {
      "src": "/api/auth(.*)",
      "dest": "/api/auth.py"
    },
    {
      "src": "/api/simple",
      "dest": "/api/simple.py"
    },
    {
      "src": "/api/leaderboard",
      "dest": "/api/leaderboard_simple.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}